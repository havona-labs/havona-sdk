# Havona API
HAVONA_API_URL=https://api.yourdomain.com

# Auth0 (from your Auth0 dashboard → Applications)
AUTH0_DOMAIN=your-tenant.us.auth0.com
AUTH0_AUDIENCE=https://api.yourdomain.com
AUTH0_CLIENT_ID=your_spa_client_id

# User credentials (password grant)
HAVONA_EMAIL=trader@yourdomain.com
HAVONA_PASSWORD=your_password_here

# M2M credentials (optional — for service accounts)
AUTH0_M2M_CLIENT_ID=your_m2m_client_id
AUTH0_M2M_CLIENT_SECRET=your_m2m_client_secret
